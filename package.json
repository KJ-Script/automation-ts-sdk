{
  "name": "automation-ts-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for automation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test:dom": "npm run build && node dist/examples/test-dom.js",
    "test:actions": "ts-node examples/test-actions.ts",
    "demo": "npm run build && node dist/examples/demo-actions.js",
    "reddit": "npm run build && node dist/examples/reddit-signin.js",
    "extract": "npm run build && node dist/examples/data-extraction.js",
    "log-json": "npm run build && node dist/examples/simple-json-logging.js",
    "ai-basic": "npm run build && node dist/examples/ai-agent-basic.js",
    "ai-chat": "npm run build && node dist/examples/ai-agent-chat.js",
    "ai-advanced": "npm run build && node dist/examples/ai-agent-advanced.js",
    "ai-conversation": "npm run build && node dist/examples/ai-agent-advanced.js conversation",
    "ai-test": "npm run build && node dist/examples/ai-agent-test.js",
    "ai-adaptive": "npm run build && node dist/examples/ai-agent-adaptive-demo.js",
    "ai-screenshot": "npm run build && node dist/examples/ai-agent-screenshot-demo.js",
    "ai-text-click": "npm run build && node dist/examples/ai-agent-text-click-demo.js",
    "ai-fast": "npm run build && node dist/examples/ai-agent-fast-demo.js",
    "ai-screenshot-test": "npm run build && node dist/examples/ai-agent-screenshot-test.js",
    "twitter": "npm run build && node dist/examples/twitter-example.js",
    "session-demo": "npm run build && node dist/examples/session-persistence-example.js",
    "test-session": "npm run build && node dist/examples/test-session-persistence.js"
  },
  "keywords": [
    "typescript",
    "sdk",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.0.0",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist"
  ]
}
